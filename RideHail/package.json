{
  "name": "com.anum.ridehail",
  "version": "1.0.0",
  "description": "Backend for ride hail type application - A practice",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "start": "tsc && node build/server.js",
    "docker-dev": "nodemon ./build/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "migrate:create": "./node_modules/.bin/typeorm migration:create -n",
    "migrate:revert": "ts-node ./node_modules/.bin/typeorm migration:revert",
    "migrate:run": "ts-node ./node_modules/.bin/typeorm migration:run",
    "seed": "node build/tasks/dbseed.js",
    "worker": "tsc && node build/tasks/worker.js"
  },
  "author": "Anum Amin",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^16.1.8",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.8",
    "@types/hapi__joi": "^16.0.4",
    "@types/pg": "^7.11.2",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "bull": "^3.12.1",
    "bull-arena": "^2.6.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "p-iteration": "^1.1.8",
    "pg": "^7.14.0",
    "redis": "^2.8.0",
    "redis-messaging-manager": "^1.6.2",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^2.3.0",
    "ts-node": "^8.5.4",
    "typeorm": "0.2.25",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@types/bull": "^3.10.6",
    "@types/bull-arena": "^2.6.0",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/redis": "^2.8.14",
    "@types/socket.io": "^2.1.4",
    "nodemon": "^2.0.2"
  }
}
